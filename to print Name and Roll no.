#include<iostream>
using namespace std;
class Details
{
	private:
		int roll;
		string name;
	
	public:
		void getdata()
		{
			cout<<"Enter Your Name:";
			cin>>name;
			cout<<"Enter Your Roll Number:";
			cin>>roll;
		}
		void putdata()
		{
			cout<<"Your Name:"<<name;
			cout<<"\t Roll Number:"<<roll<<endl<<endl;
		}
};
 
int main()
{
	Details std1,std2,std3;
	std1.getdata();
	std2.getdata();
	std3.getdata();
	cout<<endl<<endl;
	std1.putdata();
	std2.putdata();
	std3.putdata();
}
